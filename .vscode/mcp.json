{
  "servers": {
    "context7-latest": {
      "type": "stdio",
      "command": "npx",
      "args": [
        "-y",
        "@upstash/context7-mcp"
      ]
    },
    "sequentialthinking": {
      "command": "docker",
      "args": [
        "run",
        "--rm",
        "-i",
        "mcp/sequentialthinking"
      ]
    },
    "mcp-compass": {
      "command": "node",
      "args": [
        "~/mcp/mcp-compass/build/index.js"
      ]
    },
    "desktop-commander": {
      "command": "npx",
      "args": [
        "-y",
        "@wonderwhy-er/desktop-commander"
      ]
    },
    "ddg-search": {
      "command": "uvx",
      "args": [
        "duckduckgo-mcp-server"
      ]
    },
    "git-mcp-server": {
      // Use a descriptive name
      "type": "stdio",
      "command": "node", // Use node to run the script
      "args": ["~/mcp/git-mcp-server/dist/index.js"], // Relative path using home directory
    }
  },
}

