{
  "servers": {
    "context7-latest": {
      "type": "stdio",
      "command": "npx",
      "args": [
        "-y",
        "@upstash/context7-mcp"
      ]
    },
    "sequentialthinking": {
      "command": "docker",
      "args": [
        "run",
        "--rm",
        "-i",
        "mcp/sequentialthinking"
      ]
    },
    "mcp-compass": {
      "command": "node",
      "args": [
        "~/mcp/mcp-compass/build/index.js"
      ]
    },
    "desktop-commander": {
      "command": "npx", 
      "args": [
        "-y",
        "@wonderwhy-er/desktop-commander"
      ]
    },
    "ddg-search": {
      "command": "uvx",
      "args": [
        "duckduckgo-mcp-server"
      ]
    },
    "git-mcp-server": {
      "type": "stdio",
      "command": "node",
      "args": ["~/mcp/git-mcp-server/dist/index.js"]
    },
    "platform-tools": {
      "type": "stdio", 
      "command": "node",
      "args": ["~/mcp/platform-tools/dist/index.js"],
      "description": "Cross-platform package management and system tools"
    }
  }
}

